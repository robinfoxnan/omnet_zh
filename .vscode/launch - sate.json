{

    "version": "0.2.0",
    "configurations": [
        {
            "name": "debug_satellite",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}\\bin\\osg-satellites_dbg.exe",
            "args": ["-f", "${workspaceFolder}\\samples\\osg-satellites\\omnetpp.ini",
                     "-c", "BDS",
                     "-u", "Qtenv",
                     "-n", "${workspaceFolder}\\samples\\osg-satellites"
                    ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "miDebuggerPath": "D:\\omnetpp562\\tools\\win64\\mingw64\\bin\\gdb.exe",
            "setupCommands": [
                {
                    "description": "为 gdb 启用整齐打印",
